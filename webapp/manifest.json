{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "QuickStartApplication",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "",
		"description": "",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"mainService": {
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				},
				"uri": "/sap/opu/odata/sap/ZMARKETLIST_SRV/"
			},
			"ZMARKETLIST_SRV": {
				"uri": "/sm1/sap/opu/odata/sap/ZMARKETLIST_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZMARKETLIST_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "sap.ui.demo.masterdetail.view.FullApp",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.masterdetail.i18n.i18n"
				}
			},
			"": {
				"dataSource": "ZMARKETLIST_SRV"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.demo.masterdetail.view",
				"controlId": "idApp",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"master",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "default",
					"target": [
						"home"
					]
				},
				{
					"pattern": "master/plantId/:plantId:/ccId/:ccId:",
					"name": "master",
					"target": [
						"object",
						"master"
					]
				},
				{
					"pattern": "mastermobile/plantId/:plantId:/ccId/:ccId:",
					"name": "mastermobile",
					"target": [
						"mobile"
					]
				},
				{
					"pattern": "masterpage/plantId/:plantId:/ccId/:ccId:",
					"name": "masterpage",
					"target": [
						"masterpage"
					]
				},
				{
					"pattern": "submasterpage/plantId/:plantId:/groupId/:groupId:/ccId/:ccId:",
					"name": "submasterpage",
					"target": [
						"submasterpage"
					]
				},
				{
					"pattern": "home",
					"name": "home",
					"target": [
						"home"
					]
				},
				{
					"pattern": "submaster/plantId/:plantId:/groupId/:groupId:",
					"name": "submaster",
					"target": [
						"object",
						"submaster"
					]
				},
				{
					"pattern": "submaster/plantId/:plantId:/groupId/:groupId:/ccId/:ccId:",
					"name": "submaster",
					"target": [
						"object",
						"submaster"
					]
				},
				{
					"pattern": "submobile/plantId/:plantId:/groupId/:groupId:/ccId/:ccId:",
					"name": "submobile",
					"target": [
						"submaster",
						"mobile"
					]
				}
			],
			"targets": {
				"home": {
					"viewName": "homeView",
					"viewLevel": 1,
					"viewId": "home"
				},
				"splitapp": {
					"viewName": "SplitApp",
					"viewLevel": 1,
					"controlId": "idApp",
					"controlAggregation": "pages",
					"viewId": "splitapp"
				},
				"master": {
					"parent": "splitapp",
					"viewName": "Master",
					"viewLevel": 1,
					"controlId": "idAppControl",
					"viewId": "master",
					"controlAggregation": "masterPages"
				},
				"submaster": {
					"parent": "splitapp",
					"viewName": "SubMaster",
					"viewLevel": 1,
					"viewId": "submaster",
					"controlId": "idAppControl",
					"controlAggregation": "masterPages"
				},
				"masterpage": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "masterpage"
				},
				"submasterpage": {
					"viewName": "SubMaster",
					"viewLevel": 1,
					"viewId": "submasterpage"
				},
				"object": {
					"parent": "splitapp",
					"viewName": "DMarketList",
					"viewId": "detail",
					"controlId": "idAppControl",
					"controlAggregation": "detailPages",
					"viewLevel": 2
				},
				"mobile": {
					"viewName": "MMarketList",
					"viewId": "detail",
					"viewLevel": 2
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewId": "detailObjectNotFound"
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewId": "detailNoObjectsAvailable"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zmarketlist/webapp",
		"_version": "1.1.0"
	}
}